<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mvargas.examen_carrito" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name=".session.Session">
        <activity
            android:name=".Splash_Activity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_splash_" >
        </activity>
        <activity
            android:name=".RegistraActivity"
            android:label="@string/title_activity_registra" >
        </activity>
        <activity
            android:name=".CatalogoActivity"
            android:label="@string/title_activity_catalogo" >
        </activity>
        <activity
            android:name=".DetalleActivity"
            android:label="@string/title_activity_detalle" >
        </activity>
        <activity
            android:name=".CarritoActivity"
            android:label="@string/title_activity_carrito" >
        </activity>
    </application>

</manifest>
